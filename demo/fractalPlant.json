{
    "variables" : ["X","F"],
    "constants" : ["+","-","[","]"],
    "axiom" : "X",
    "rules" : {
        "X" : "F+[[X]-X]-F[-FX]+X",
        "F" : "FF"
    },
    "translations" : {
